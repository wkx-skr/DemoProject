    "test:e2e": "vue-cli-service test:e2e",
    "lint": "eslint --fix --ext .js,.ts,.vue src",
    "dev": "node src/utils/os.js & vue-cli-service serve --mode development",
    "dev:ddd": "node src/utils/os.js & cross-env NODE_APP=ddd vue-cli-service serve --mode development",
    "electron:build": "vue-cli-service electron:build",
    "electron:build:mac": "vue-cli-service electron:build --mac",
    "electron:build:linux": "vue-cli-service electron:build --linux --x64",
    "electron:build:linux:arm": "vue-cli-service electron:build --linux --arm64",
    "electron:serve": "vue-cli-service electron:serve",
    "lib": "cross-env COMMON_LIB=ER vue-cli-service build --target lib -dest lib --name modelEdit ./src/views/list/index.js"
