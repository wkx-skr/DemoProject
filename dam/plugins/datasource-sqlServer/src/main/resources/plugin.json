{
  "datasourceType": "SQLSERVER",
  "displayName": "SQL Server",
  "icon": "sqlserver",
  "datasourceClass": "com.datablau.datasources.sqlServer.SqlServerDatasource",
  "jdbcDriverClass": "com.microsoft.sqlserver.jdbc.SQLServerDriver",
  "suggestVersion": "2008、2012、2014、2019",
  "build": "${build.timestamp}",
  "options": [
    {
      "code": "HostServer",
      "defaultValue": "localhost",
      "required": true,
      "requiredOnSave": true
    },
    {
      "code": "PortNumber",
      "defaultValue": "1433",
      "required": true,
      "requiredOnSave": true
    },
    {
      "code": "AuthenticationType",
      "candidate": [
        "password"
      ],
      "defaultValue": 0,
      "required": false
    },
    {
      "code": "DatabaseName",
      "defaultValue": "",
      "candidate": [],
      "requiredOnSave": true
    },
    {
      "code": "SelectedSchemas",
      "multiple": true,
      "candidate": [],
      "requiredOnSave": true
    }
  ]
}