  "icon": "vertica",
  "datasourceClass": "com.datablau.datasources.vertica.VerticaDatasource",
  "jdbcDriverClass": "com.vertica.jdbc.Driver",
  "suggestVersion": "9.3.1-4",
  "build": "${build.timestamp}",
  "options": [
    {
      "code": "HostServer",
      "defaultValue": "localhost",
