        "password"
      ],
      "defaultValue": 0,
      "required": false
    },
    {
      "code": "DatabaseName",
      "defaultValue": "",
      "candidate": [
      ],
      "required": false,
      "requiredOnSave": true
    },
    {
      "code": "SelectedSchemas",
      "multiple": true,
      "required": false,
      "candidate": [
      ],
      "requiredOnSave": true
    }
  ]
