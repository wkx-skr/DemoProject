{
  "datasourceType": "TIDB",
  "displayName": "TiDB",
  "icon": "tidb",
  "datasourceClass": "com.datablau.datasources.tidb.TidbDatasource",
  "jdbcDriverClass": "com.mysql.jdbc.Driver",
  "build": "${build.timestamp}",
  "options": [
    {
      "code": "HostServer",
      "defaultValue": "localhost",
      "required": true,
      "requiredOnSave": true
    },
    {
      "code": "PortNumber",
      "defaultValue": "4000",
      "required": true,
      "requiredOnSave": true
    },
    {
      "code": "AuthenticationType",
      "candidate": [
        "password"
      ],
      "defaultValue": 0,
      "required": false
    },
    {
      "code": "DatabaseName",
      "defaultValue": "",
      "required": false,
      "multiple": true,
      "candidate": [],
      "requiredOnSave": true
    }
  ]
}