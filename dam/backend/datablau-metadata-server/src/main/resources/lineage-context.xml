  <bean id="mysqlParser" class="com.andorj.lineage.sql.mysql.MySqlParser" />
  <bean id="dataXCommonParse" class="com.andorj.lineage.datax.parse.DataXCommonParse" />
  <bean id="clickHouseScriptParser" class="com.andorj.lineage.sql.clickhouse.ClickHouseScriptParser" />
