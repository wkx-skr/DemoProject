<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="kettleLineageParser" class="com.andorj.lineage.kettle.KettleParser"/>
  <bean id="tableauLineageParser" class="com.andorj.lineage.tableau.TableauParser"/>
  <bean id="datastageLineageParser" class="com.andorj.lineage.datastage.DataStageParser"/>
  <bean id="ssisLineageParser" class="com.andorj.lineage.ssis.SSISParser"/>
  <bean id="restCloudParser" class="com.andorj.lineage.restcloud.RestCloudParser"/>
  <bean id="genSqlParser" class="com.andorj.lineage.generalsql.GeneralSqlParser"/>
  <bean id="informaticParser" class="com.andorj.lineage.informatica.InformaticaParser"/>
  <bean id="sqlServerTsqlParser" class="com.andorj.lineage.sql.sqlserver.SqlServerTSqlParser"/>
  <bean id="plSqlParser" class="com.andorj.lineage.sql.plsql.PlSqlParser"/>
  <bean id="postgreSqlParser" class="com.andorj.lineage.sql.postgresql.PostgreSqlParser"/>
